<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>io.goauthentik.platform.sysd</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Applications/authentik Agent.app/Contents/Resources/ak-sysd</string>
        <string>--config-file</string>
        <string>/opt/authentik/config.json</string>
	</array>
	<key>UserName</key>
	<string>root</string>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
    <key>StandardOutPath</key>
    <string>/Library/Logs/io.goauthentik/sysd.log</string>
    <key>StandardErrorPath</key>
    <string>/Library/Logs/io.goauthentik/sysd.log</string>
</dict>
</plist>
