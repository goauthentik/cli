{
  "manifest_version": 3,
  "name": "authentik Extension",
  "version": "0.12.5",
  "description": "authentik Browser extension to launch applications.",
  "permissions": [
    "storage",
    "activeTab",
    "nativeMessaging"
  ],
  "background": {
    "service_worker": "dist/background.js",
    "type": "module"
  },
  "omnibox": {
    "keyword": "ak"
  },
  "options_ui": {
    "page": "src/options.html",
    "open_in_tab": false
  },
  "host_permissions": [
    "https://*/"
  ],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
