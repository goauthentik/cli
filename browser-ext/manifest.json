{
  "manifest_version": 3,
  "name": "authentik Launcher",
  "version": "1.0",
  "description": "A quick application launcher for applications in an authentik instance.",
  "permissions": ["storage", "activeTab", "nativeMessaging"],
  "background": {
    "service_worker": "dist/background.js",
    "type": "module"
  },
  "omnibox": {
    "keyword": "ak"
  },
  "host_permissions": ["https://*/"],
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
